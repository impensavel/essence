    "keywords": [
        "essence",
        "data",
        "extraction",
        "csv",
        "xml"
    ],
    "homepage": "https://github.com/impensavel/essence",
    "license": "MIT",
    "authors": [
        {
            "name": "Quetzy Garcia",
            "email": "quetzyg@impensavel.com"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-soap": "*",
        "ext-xmlreader": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Impensavel\\Essence\\": "src/"
